#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Storage/Local Web/lftd/node_modules/.pnpm/errno@0.1.8/node_modules/errno/node_modules:/Volumes/Storage/Local Web/lftd/node_modules/.pnpm/errno@0.1.8/node_modules:/Volumes/Storage/Local Web/lftd/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/Storage/Local Web/lftd/node_modules/.pnpm/errno@0.1.8/node_modules/errno/node_modules:/Volumes/Storage/Local Web/lftd/node_modules/.pnpm/errno@0.1.8/node_modules:/Volumes/Storage/Local Web/lftd/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../errno/cli.js" "$@"
else
  exec node  "$basedir/../errno/cli.js" "$@"
fi
